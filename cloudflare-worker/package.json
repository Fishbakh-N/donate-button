{
  "name": "donate-button-cloudflare-worker",
  "version": "1.0.0",
  "description": "Cloudflare worker for serving the Every.org donate button at assets.every.org",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "dev": "NODE_ENV=development npm run build",
    "format": "prettier --write '**/*.{ts,js,css,json,md}'",
    "test:clean": "rimraf ./transpiled/src ./transpiled/test",
    "test": "ava",
    "transpile": "tsc --project ./test"
  },
  "author": "author",
  "license": "MIT OR Apache-2.0",
  "devDependencies": {
    "@cloudflare/workers-types": "^2.1.0",
    "@types/service-worker-mock": "^2.0.1",
    "@types/sinon": "^9.0.9",
    "ava": "^3.14.0",
    "rimraf": "^3.0.2",
    "service-worker-mock": "^2.0.5",
    "sinon": "^9.2.2",
    "ts-loader": "^7.0.5",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "xo": "^0.36.1"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register",
      "./test/_setup.ts"
    ]
  },
  "dependencies": {
    "fp-ts": "^2.9.1",
    "io-ts": "^2.2.13",
    "path-to-regexp": "^6.2.0"
  }
}
